t <- wilcox.test( jitter(data1), jitter(data2) )
d[i] <- t$p.value
s[i] <- t$statistic
}
list(test='wilcox.test n=1000', comparing = paste( colA, colB, sep=" vs. "),  mean.p.value =mean (d),mean.estimate="Only correlations", mean.statistics=mean(s), figure=fn)
}
